<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.robinsonprogramming.www.loginrestapi.AddService">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#2d2d2d"
        android:orientation="vertical"
        android:paddingLeft="24dp"
        android:paddingRight="24dp"
        android:paddingTop="56dp"
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="16dp">


        <ImageButton
            android:id="@+id/imageButtonAdd1"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:background="#2d2d2d"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@mipmap/twitter96" />

        <ImageButton
            android:id="@+id/imageButtonAdd2"
            android:layout_below="@+id/imageButtonAdd1"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:background="#2d2d2d"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@mipmap/browser96" />

        <ImageButton
            android:id="@+id/imageButtonAdd3"
            android:layout_below="@+id/imageButtonAdd1"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:background="#2d2d2d"
            android:layout_gravity="center_horizontal"
            app:srcCompat="@mipmap/new96" />

    </LinearLayout>
</android.support.constraint.ConstraintLayout>
